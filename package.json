{
  "name": "plume",
  "productName": "ragekit.plume",
  "description": "My Electron application description",
  "keywords": [],
  "main": "./main.js",
  "version": "1.0.0",
  "author": "bgatt",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build" : "build"
  },
  "dependencies": {},
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.34",
    "@electron-forge/maker-deb": "6.0.0-beta.34",
    "@electron-forge/maker-rpm": "6.0.0-beta.34",
    "@electron-forge/maker-squirrel": "6.0.0-beta.34",
    "@electron-forge/maker-zip": "6.0.0-beta.34",
    "electron": "5.0.1",
    "electron-builder": "^20.41.0"
  },
  "build": {
    "appId": "ragekit.plume",
    "productName": "Plume",
    "win": {
      "target": ["portable"]
    },
    "portable": {
      "artifactName": "plume_portable.exe"
  },
  "directories": {
    "output": "out",
    "buildResources": "out/buildResources"
  }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ]
    }
  }
}
